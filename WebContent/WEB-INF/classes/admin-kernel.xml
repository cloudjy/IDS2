<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="admin-kernel" namespace="/admin" extends="admin-default">
		<action name="kernel" class="com.gnamp.struts.admin.action.KernelAction">
			<result name="success">/admin/kernel/list.jsp</result>
			<result name="add">/admin/kernel/add.jsp</result>
			<result name="preedit">/admin/kernel/edit.jsp</result>
		</action>
		<action name="kernelupload" class="com.gnamp.struts.admin.action.FlexKernelUploadAction">
			<result name="success">/admin/kernel/flexupload.jsp</result>
		</action>
		<action name="jkernelupload" class="com.gnamp.struts.admin.action.KernelUploadAction">
			<result name="success">/admin/kernel/upload.jsp</result>
		</action>
	</package>
</struts>
